{
  "name": "aoc-25",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "aoc-25-03",
    "aoc-25-05",
    "aoc-25-06",
    "aoc-25-07"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
